/',\r\n  STAT: 'http://stat.api.4399.com/'\r\n}\r\n// 根据发布环境切换域名\r\nvar curDomain = BuildConfig.compare(BuildConfig.PRE) ? baseDomain : testDomain\r\n\r\nexport default {\r\n  SCORE: {\r\n    // 提交积分\r\n    SUBMIT: curDomain.SCORE + 'submit',\r\n    // 获得排行榜\r\n    GET_RANK: curDomain.SCORE + 'getRank'\r\n  },\r\n  AD: {\r\n    // 广告信息\r\n    INFO: curDomain.STAT + 'xml.php'\r\n  }\r\n}\r\n\n\n\n// WEBPACK FOOTER //\n// ./src/store/constant/request-urls.js","import Vue from 'vue'\r\n// 字符转换\r\nimport {\r\n  stringify\r\n} from 'qs'\r\nimport md5 from 'js-md5'\r\nimport {\r\n  parseString\r\n} from 'xml2js'\r\n// 地址\r\nimport Url from '../constant/request-urls'\r\n\r\n// 请求超时为15秒\r\nVue.axios.defaults.timeout = 15000\r\n// 设置自动带cookie\r\nVue.axios.defaults.withCredentials = true\r\n\r\nvar config = {\r\n  headers: {\r\n    'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8'\r\n  }\r\n}\r\n// 命令\r\nexport const RequestAction = {\r\n  // 获得游戏信息\r\n  GET_GAME_INFO: 'REQUEST_GET_GAME_INFO',\r\n  // 提交积分\r\n  SUBMIT_SCORE: 'REQUEST_SUBMIT_SCORE',\r\n  // 获得排行榜\r\n  GET_RANK: 'REQUEST_GET_RANK',\r\n  // 获得广告信息\r\n  GET_AD_INFO: 'REQUEST_GET_AD_INFO'\r\n}\r\n\r\n// 命令方法\r\nconst actions = {\r\n  // 获得游戏信息\r\n  [RequestAction.GET_GAME_INFO]({\r\n    dispatch\r\n  }) {\r\n    Vue.console.info('解析XML获取游戏基本信息')\r\n    return new Promise((resolve, reject) => {\r\n      Vue.axios.get('./static/base.xml').then(res => {\r\n        parseString(res.data, {\r\n          explicitArray: false,\r\n          ignoreAttrs: true\r\n          // eslint-disable-next-line\r\n        }, (err, data) => {\r\n          resolve(data.adstore)\r\n        })\r\n      }).catch(e => {\r\n        Vue.console.error(e)\r\n        Vue.console.error('请求游戏基本信息报错，游戏已引入原创API，请尽快上传原创平台预览')\r\n      })\r\n    })\r\n  },\r\n  // 提交积分\r\n  [RequestAction.SUBMIT_SCORE]({\r\n    dispatch,\r\n    rootState\r\n  }, data) {\r\n    Vue.console.log('请求提交积分', data)\r\n    let gameKey = md5(`html5api${rootStat