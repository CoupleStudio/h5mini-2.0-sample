      let div = document.getElementById('adContainer')\r\n      let adContainer = new window.google.ima.AdDisplayContainer(div)\r\n      adContainer.initialize()\r\n      let adsLoader = new window.google.ima.AdsLoader(adContainer)\r\n      let adsManager\r\n      let onAdError = adErrorEvent => {\r\n        this.$console.error('广告播放失败')\r\n        console.log(adErrorEvent.getError())\r\n        if (adsManager) {\r\n          adsManager.destroy()\r\n          this.$store.dispatch(StatisticAction.AD_REQUEST, '加载SDK失败')\r\n        } else {\r\n          this.$store.dispatch(StatisticAction.AD_REQUEST, '加载资源失败')\r\n        }\r\n        this.$store.dispatch(AdAction.PLAY_COMPLETE, false)\r\n      }\r\n      adsLoader.addEventListener(window.google.ima.AdsManagerLoadedEvent.Type.ADS_MANAGER_LOADED, event => {\r\n        let settings = new window.google.ima.AdsRenderingSettings()\r\n        settings.restoreCustomPlaybackStateOnAdBreakComplete = true\r\n        adsManager = event.getAdsManager(settings)\r\n        /*\r\n        AdEvent.Type.CLICK 在用户点击广告时触发。\r\n        AdEvent.Type.LOADED 在有广告数据时触发。\r\n        AdEvent.Type.START 在广告开始播放时触发。\r\n        AdEvent.Type.USER_CLOSE 在用户关闭广告时触发。\r\n        */\r\n        adsManager.addEventListener(window.google.ima.AdErrorEvent.Type.AD_ERROR, onAdError)\r\n        adsManager.addEventListener(window.google.ima.AdEvent.Type.CLICK, this.onAdEvent)\r\n        adsManager.addEventListener(window.google.ima.AdEvent.Type.USER_CLOSE, this.onAdEvent)\r\n        adsManager.addEventListener(window.google.ima.AdEvent.Type.LOADED, this.onAdEvent)\r\n        adsManager.addEventListener(window.google.ima.AdEvent.Type.STARTED, this.onAdEvent)\r\n        try {\r\n          adsManager.init(this.winWidth, this.winHeight, window.google.ima.ViewMode.FULLSCREEN)\r\n          adsManager.start()\r\n        } catch (adError) {\r\n          this.$store.dispatch(StatisticAction.AD_REQUEST, '调用SDK失败')\r\n        }\r\n      }, false)\r\n      adsLoader.addEventListener(window.google.ima.AdErrorEvent.Type.AD_ERROR, onAdError, false)\r\n      let adsRequest = new window.google.ima.AdsRequest()\r\n      adsRequest.adTagUrl = `https://googleads.g.doubleclick.net/pagead/ads?ad_type=standardvideo_image&client=${this.pubid}&description_url=http%3A%2F%2Fwww.4399.com&channel=${this.channelId}&videoad_start_delay=15000&hl=zh_CN&max_ad_duration=15000`\r\n      adsRequest.linearAdSlotWidth = adsRequest.nonLinearAdSlotWidth = this.winWidth\r\n      adsRequest.linearAdSlotHeight = adsRequest.nonLinearAdSlotHeight = this.winHeight\r\n      adsRequest.forceNonLinearFullSlot = true\r\n      adsLoader.requestAds(adsRequest)\r\n    },\r\n    onAdEvent(adEvent) {\r\n      switch (adEvent.type) {\r\n        case window.google.ima.AdEvent.Type.LOADED:\r\n          this.$store.dispatch(StatisticAction.AD_PLAY, '加载完成')\r\n          break\r\n        case window.google.ima.AdEvent.Type.STARTED:\r\n          this.$store.dispatch(StatisticAction.AD_PLAY, '开始')\r\n          break\r\n        case window.google.ima.AdEvent.Type.CLICK:\r\n          this.$store.dispatch(StatisticAction.AD_PLAY, '点击广告')\r\n          break\r\n        case window.google.ima.AdEvent.Type.USER_CLOSE:\r\n          this.$store.dispatch(StatisticAction.AD_PLAY, '关闭')\r\n          this.$store.dispatch(AdAction.PLAY_COMPLETE, true)\r\n          break\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\n\n\n// WEBPACK FOOTER //\n// src/components/Ad.vue","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\"div\", {\n    staticClass: \"ad-container\",\n    attrs: { id: \"adContainer\" }\n  })\n}\nvar staticRenderFns = []\nrender._withStripped = true\nvar esExports = { render: render, staticRenderFns: staticRenderFns }\nexport default esExports\nif (module.hot) {\n  module.hot.accept()\n  if (module.hot.data) {\n    require(\"vue-loader/node_modules/vue-hot-reload-api\")      .rerender(\"data-v-5076c29a\", esExports)\n 